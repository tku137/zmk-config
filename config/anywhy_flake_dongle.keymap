#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    /* This empty keymap is for the dongle build.
     * It allows the dongle to have a valid firmware
     * without containing any key bindings itself.
     */
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <&trans>;
        };
    };
};
